[Unit]
Description=Docker
[Service]
ExecStart=/usr/bin/dockerd \
--bip=<CHANGE ME> \
--host=tcp://0.0.0.0:2375 \
--host=unix:///var/run/docker.sock \
--cluster-store=etcd://10.5.5.11:2379 \
--cluster-advertise=10.5.5.11:2375 \
--storage-driver=overlay
Restart=on-failure
RestartSec=5
[Install]
WantedBy=multi-user.target

